{
  "name": "nutriasoft.dev",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "preview": "astro preview",
    "prepare": "husky install .husky",
    "build": "astro check && astro build",
    "lint": "eslint --fix {src,scripts}/**/*.{astro,js,ts}",
    "format": "prettier --write {src,scripts}/**/*.{astro,js,ts}"
  },
  "engines": {
    "node": ">=20.12.0",
    "pnpm": ">=8.15.0"
  },
  "dependencies": {
    "@astrojs/vercel": "7.8.2",
    "astro": "4.12.3"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/tailwind": "5.1.2",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "astro-icon": "1.1.1",
    "astro-seo": "0.8.4",
    "eslint": "8.57.1",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-astro": "0.34.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.8",
    "standard": "17.1.2",
    "tailwindcss": "3.4.14",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "*.{astro,ts}": [
      "pnpm format",
      "pnpm lint"
    ]
  }
}