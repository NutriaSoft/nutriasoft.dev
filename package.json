{
  "name": "nutriasoft.dev",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "preview": "astro preview",
    "prepare": "husky install .husky",
    "build": "astro check && astro build",
    "lint": "eslint --fix {src,scripts}/**/*.{astro,js,ts}",
    "format": "prettier --write {src,scripts}/**/*.{astro,js,ts}"
  },
  "engines": {
    "node": ">=20.18.0",
    "pnpm": ">=8.15.0"
  },
  "dependencies": {
    "@astrojs/vercel": "8.0.1",
    "astro": "5.1.2"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/tailwind": "5.1.4",
    "@typescript-eslint/eslint-plugin": "8.19.0",
    "@typescript-eslint/parser": "8.19.0",
    "astro-icon": "1.1.5",
    "astro-seo": "0.8.4",
    "eslint": "9.17.0",
    "eslint-config-love": "114.0.0",
    "eslint-plugin-astro": "1.3.1",
    "husky": "8.0.3",
    "lint-staged": "15.3.0",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.9",
    "standard": "17.1.2",
    "tailwindcss": "3.4.17",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "*.{astro,ts}": [
      "pnpm format",
      "pnpm lint"
    ]
  }
}