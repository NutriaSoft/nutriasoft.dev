{
  "name": "nutriasoft.dev",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "preview": "astro preview",
    "prepare": "husky install .husky",
    "build": "astro check && astro build",
    "lint": "eslint --fix {src,scripts}/**/*.{astro,js,ts}",
    "format": "prettier --write {src,scripts}/**/*.{astro,js,ts}"
  },
  "engines": {
    "node": ">=20.18.0",
    "pnpm": ">=8.15.0"
  },
  "dependencies": {
    "@astrojs/vercel": "8.1.5",
    "astro": "5.9.0",
    "wc-toast": "1.3.1"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@tailwindcss/vite": "4.1.8",
    "@typescript-eslint/eslint-plugin": "8.33.1",
    "@typescript-eslint/parser": "8.33.1",
    "astro-icon": "1.1.5",
    "eslint": "9.28.0",
    "eslint-config-love": "120.0.0",
    "eslint-plugin-astro": "1.3.1",
    "husky": "8.0.3",
    "lint-staged": "15.5.2",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.12",
    "standard": "17.1.2",
    "tailwindcss": "4.1.8",
    "typescript": "5.8.3"
  },
  "lint-staged": {
    "*.{astro,ts}": [
      "pnpm format",
      "pnpm lint"
    ]
  }
}